# =============================================================================
# FIXER POST APP ENVIRONMENT VARIABLES
# =============================================================================
# Copy this file to .env and fill in your actual values

# =============================================================================
# API CONFIGURATION
# =============================================================================
VITE_API_BASE_URL=http://localhost:3001/api/v1
VITE_API_TIMEOUT=30000

# =============================================================================
# SUPABASE CONFIGURATION
# =============================================================================
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your-anon-key-here

# =============================================================================
# STRIPE CONFIGURATION
# =============================================================================
VITE_STRIPE_PUBLISHABLE_KEY=pk_test_your-publishable-key-here

# =============================================================================
# MAPBOX CONFIGURATION
# =============================================================================
VITE_MAPBOX_ACCESS_TOKEN=pk.your-mapbox-token-here

# =============================================================================
# APPLICATION SETTINGS
# =============================================================================
VITE_APP_NAME=Fixer Post
VITE_APP_DESCRIPTION=Post jobs and find skilled workers
VITE_APP_VERSION=1.0.0

# =============================================================================
# FEATURE FLAGS
# =============================================================================
VITE_ENABLE_ANALYTICS=false
VITE_ENABLE_ERROR_REPORTING=false
VITE_ENABLE_DEBUG=false

# =============================================================================
# EXTERNAL SERVICES
# =============================================================================
# Google Analytics
VITE_GA_TRACKING_ID=

# Sentry (Error Reporting)
VITE_SENTRY_DSN=

# Intercom (Customer Support)
VITE_INTERCOM_APP_ID=

# =============================================================================
# DEVELOPMENT SETTINGS
# =============================================================================
VITE_MOCK_API=false
VITE_SHOW_DEV_TOOLS=true
